{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "proxies": [
      {
        "source": "/api/(.*)",
        "destination": "https://api.echina.mn/api/$1"
      },
      {
        "source": "/api/search/(.*)",
        "destination": "http://api.airkacc.mn/api/search/$1"
      },
      {
        "source": "/api/prebooking/(.*)",
        "destination": "http://api.airkacc.mn/api/prebooking/$1"
      }
    ]
  }
  