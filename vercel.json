{
    "version": 2,
    "redirects": [
      {
        "source": "/api/(.*)",
        "destination": "https://api.echina.mn/api/$1",
        "permanent": true
      },
      {
        "source": "/api/search/(.*)",
        "destination": "https://api.airkacc.mn/api/search/$1",
        "permanent": true
      },
      {
        "source": "/api/prebooking/(.*)",
        "destination": "https://api.airkacc.mn/api/prebooking/$1",
        "permanent": true
      }
    ]
  }
  